{
  "hosting": {
    "public": "build",
    "ignore": [
      "firebase.json",
      "**/.*",
      "**/node_modules/**"
    ],
    "rewrites": [ 
      {
		  "source": "/tracks/**", "destination": "/tracks/"
      },
      {
		  "source": "!/tracks/**", "destination": "/index.html"
      }
    ]
  }
}
